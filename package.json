{
  "name": "gatsby-starter-tailwind",
  "description": "Gatsby starter styled with Tailwind",
  "version": "2.1.0",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "babel-plugin-styled-components": "^1.10.0",
    "devicon": "^2.0.0",
    "gatsby": "^2.1.4",
    "gatsby-plugin-manifest": "^2.0.17",
    "gatsby-plugin-postcss": "^2.0.5",
    "gatsby-plugin-purgecss": "^3.1.0",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-styled-components": "^3.0.6",
    "graphql": "^14.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf public .cache",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "serve:build": "npm run clean && npm run build && http-server ./public",
    "start:prod": "npm run build && npm run serve:build",
    "deploy:github": "npm run clean && npm run build && node ./scripts/deploy-github",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "devDependencies": {
    "prettier": "^1.16.4",
    "tailwindcss": "^0.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind"
  },
  "bugs": {
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind/issues"
  }
}
